<?xml version="1.0" encoding="UTF-8"?>
<greenHouse
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.example.org/greenHouse"
    xsi:schemaLocation="https://github.com/AnnaDan1347/XMLStudy/blob/master/eclipse-workspace/XMLStudy/src/epam/by/greenHouse.xsd">
    <plant id="a&amp;1">
        <name>rose</name>
        <soil>SOD_PODZOLIC</soil>
        <origin>Netherlands</origin>
        <visual>
            <stemColor>dark green</stemColor>
            <leafColor>dark green</leafColor>
            <averagePlantSize>30</averagePlantSize>
        </visual>
        <growingTips>
            <temperatureDegree>15</temperatureDegree>
            <lighting>LIGHT_REQUIRING</lighting>
            <wateringMlPerWeek>300</wateringMlPerWeek>
        </growingTips>
        <multiplying>CUTTING</multiplying>
    </plant>
    <plant id="a&amp;2">
        <name>ficus</name>
        <soil>GROUND</soil>
        <origin>Malaysia</origin>
        <visual>
            <stemColor>light brown</stemColor>
            <leafColor>light green</leafColor>
            <averagePlantSize>40</averagePlantSize>
        </visual>
        <growingTips>
            <temperatureDegree>15</temperatureDegree>
            <lighting>NOTLIGHT-REQUIRING</lighting>
            <wateringMlPerWeek>500</wateringMlPerWeek>
        </growingTips>
        <multiplying>BYLEAVES</multiplying>
    </plant>
    <plant id="a&amp;3">
        <name>muraya</name>
        <soil>PODZOLIC</soil>
        <origin>Japan</origin>
        <visual>
            <stemColor>light brown</stemColor>
            <leafColor>dark green</leafColor>
            <averagePlantSize>15</averagePlantSize>
        </visual>
        <growingTips>
            <temperatureDegree>15</temperatureDegree>
            <lighting>NOTLIGHT-REQUIRING</lighting>
            <wateringMlPerWeek>250</wateringMlPerWeek>
        </growingTips>
        <multiplying>BYSEEDS</multiplying>
    </plant>
</greenhouse>
